  "title": "Jw Subtitle",
  "description": "Fetch subtitle from jw.org video",
  "icon": "extension-icon.png",
  "author": "xellosiris",
  "platforms": [
    "macOS",
    "Windows"
  ],
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "jw-subtitle",
      "title": "Fetch Jw Subtitle",
